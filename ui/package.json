{
  "name": "@aster/ui",
  "version": "0.16.0",
  "description": "Universal AI Agent UI SDK for Aster Framework - Production Ready",
  "type": "module",
  "main": "dist/aster-ui.umd.js",
  "module": "dist/aster-ui.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/aster-ui.es.js",
      "require": "./dist/aster-ui.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -p tsconfig.check.json --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc -p tsconfig.check.json --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "ai",
    "agent",
    "chat",
    "ui",
    "vue",
    "vue3",
    "aster"
  ],
  "author": "Aster Team",
  "license": "MIT",
  "dependencies": {
    "@aster/client-js": "file:../client-sdks/client-js",
    "marked": "^17.0.1",
    "pinia": "^3.0.4",
    "vue": ">=3.3.0",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^3.2.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.28.0",
    "fast-check": "^4.5.2",
    "happy-dom": "^20.0.11",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.3",
    "vue-tsc": "^3.1.5"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  }
}
